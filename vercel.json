{
  "functions": {
    "api/gambar.js": {
      "maxDuration": 60,
      "memory": 1024
    },
    "api/*.js": {
      "maxDuration": 60,
      "memory": 1024
    }
  },
  "env": {
    "NODE_ENV": "production"
  },
  "buildCommand": "npm install --platform=linux --arch=x64 sharp",
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=86400, s-maxage=31536000, immutable"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, OPTIONS"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/image",
      "destination": "/api/gambar"
    }
  ]
}
